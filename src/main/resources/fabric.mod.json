{
	"schemaVersion": 1,
	"id": "first-mc-modd",
	"version": "${version}",
	"name": "First mc modd",
	"description": "My first mod. Built for Fabric :)",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://smlxdesign.is-a.dev",
		"sources": "https://github.com/smlxdesign/first-mc-modd"
	},
	"license": "BSD-3-Clause",
	"icon": "assets/first-mc-modd/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "smlxdesign.firstmcmodd.FirstMcModd",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "smlxdesign.firstmcmodd.FirstMcModdClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"first-mc-modd.mixins.json",
		{
			"config": "first-mc-modd.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {}
}